{
  "presets": [
    [ "@babel/preset-env", { "modules": false ,"targets": {
      "node": "current"
    }} ],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    [ "@babel/plugin-transform-arrow-functions" , { "spec": true } ],
    [ "@babel/plugin-proposal-class-properties", { "loose": true } ]
  ]
}